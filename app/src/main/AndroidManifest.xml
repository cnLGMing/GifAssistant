<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wind.gifassistant"
    android:versionCode="1"
    android:versionName="1.0">

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name="GifAssistantApp"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppBaseTheme">
        <activity
            android:name=".ui.naviwelcome.NavigationWelcomeActivity"
            android:configChanges="keyboardHidden"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ui.MainActivity"
            android:label="GifAssistant"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
            >
        </activity>

        <activity
            android:name=".ui.VideoPlayerActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
        >
        </activity>

        <activity
            android:name=".ui.GifShowActivity"
            android:configChanges="keyboardHidden|orientation"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
        >

        </activity>

        <activity
            android:name=".ui.RangSetting"
            android:label="RangSetting"
            android:theme="@style/DialogTheme.NumberPicker.Dark"
        >

        </activity>

    </application>

</manifest>
